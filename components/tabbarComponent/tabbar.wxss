.middle-wrapper {
    position: absolute;
    right: 310rpx;
    bottom: 0;
    background-color: #fff;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
}

/* .middle-wrapper.iphoneX-height {
    bottom: 66rpx;
} */

.tabbar_nav {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
    position: relative;
}

.tabbar_icon {
    width: 42rpx;
    height: 42rpx;
}

.special-wrapper {
    position: absolute;
    left: 23rpx;
    top: -42rpx;
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
    border-top: 2rpx solid #f2f2f3;
    background-color: #fff;
    text-align: center;
    box-sizing: border-box;
    padding: 12rpx;
}

.special-wrapper .tabbar_icon {
    width: 72rpx;
    height: 80rpx;
}

.special-text-wrapper {
    width: 56rpx;
    height: 56rpx;
}

.mask {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: rgba(255,255,255, 0.9);
    z-index: 1000;
    width: 100%;
    height: 100%;
    
}

.model {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    color: #fff;
    z-index: 2605;
}

